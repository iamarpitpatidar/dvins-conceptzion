// Start Case Studies
section#case-studies.lqd-section.case-studies.bg-white.rounded-10.mb-90
  .container-fluid.border-bottom.border-lightgray.py-30
    .container
      .row
        .col.col-12.text-start
          .ld-fancy-heading.relative.module-title
            h2.ld-fh-element.inline-block.relative.text-28.m-0 Recent Projects
