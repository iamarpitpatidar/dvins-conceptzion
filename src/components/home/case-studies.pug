              span.liquid-filter-items-label.size-md Filter by
              ul#pf-filter-case-studies.filter-list.filter-list-inline.items-centerd-lg-flex.flex.items-center(class='md:hidden')
                li.active.text-15(data-filter='*')
                  span All
                li.text-15(data-filter='.branding')
                  span Branding
                li.text-15(data-filter='.custom-print')
                  span Custom Print
                li.text-15(data-filter='.digital-design')
                  span Digital Design
                li.text-15(data-filter='.ecommerce')
                  span Ecommerce
              .lqd-filter-dropdown.hidden(class='md:block' data-form-options='{ "dropdownAppendTo": "self" }')
                select#lqd-filter-dropdown-pf-filter-case-studies(name='lqd-filter-dropdown-pf-filter-case-studies')
                  option(selected='' data-filter='*' value='*') All
                  option(data-filter='.branding' value='.branding') Branding
                  option(data-filter='.custom-print' value='.custom-print') Custom Print
                  option(data-filter='.digital-design' value='.digital-design') Digital Design
                  option(data-filter='.ecommerce' value='.ecommerce') Ecommerce
              a.btn.btn-naked.btn-icon-right.btn-hover-swp.text-13.font-normal.uppercase.leading-1em.text-black.rounded-100.whitespace-nowrap.capitalize(href='#contact-modal' data-lity='#contact-modal')
                span.btn-txt(data-text='Hire me') Contact Us
                span.btn-icon
                  i.lqd-icn-ess.icon-md-arrow-forward
                span.btn-icon
                  i.lqd-icn-ess.icon-md-arrow-forward
